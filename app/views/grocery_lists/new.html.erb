<% provide(:title, 'Create List') %>
<h1>Create List</h1>

<div class="row">
	<div class="span6 offset3">
		<%= form_for(@groceryList) do |f| %>
			<%= render 'shared/error_messages', object: f.object %>
			
			<%= f.label :name %>
			<%= f.text_field :name %>

			<%= f.submit "Create!", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
